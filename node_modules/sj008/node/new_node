ini node baru

// Definisikan kelas Node untuk merepresentasikan simpul dalam struktur data
class Node {
    constructor(data) {
        this.data = data; // Menyimpan data dalam simpul
        this.next = null; // Referensi ke simpul berikutnya, awalnya diatur null
    }
}

// Contoh penggunaan kelas Node
const node1 = new Node(10); // Membuat simpul dengan data 10
const node2 = new Node(20); // Membuat simpul dengan data 20

// Mengatur simpul berikutnya dari node1 ke node2
node1.next = node2;

// Output nilai data dari node1 dan node2
console.log("Data dari node1:", node1.data); // Output: 10
console.log("Data dari node2:", node1.next.data); // Output: 20